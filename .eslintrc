{
  "extends": [
    "plugin:vue/recommended"
  ],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "vue/max-attributes-per-line": ["error", {
      "singleline": 5,
      "multiline": {
        "max": 5,
        "allowFirstLine": false
      }
    }],
    "vue/html-self-closing": ["error", {
      "html": {
        "void": "always"
      }
    }]
  },
}